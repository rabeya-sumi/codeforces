#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
int main()
{
    ll t=1;
    cin>>t;
    while(t--)
    {
        int n,k;
        cin>>n>>k;
        for(int i=0 ; i<n ; i++){
            if(i==0) cout<<0<<"\n";
            else cout<<(i^(i-1))<<"\n";
 
            int match;
            cin>>match;
            if(match==1) break;
        }
    }
 
 
 
 
return 0;
}
