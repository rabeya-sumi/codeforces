#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
 
int main()
{
    int t, n, a[200005];
    cin>>t;
    while(t--){
        cin>>n;
        for(int i=0 ; i<n ; i++) cin>>a[i];
        sort(a, a+n, greater<int>());
        ll alice=0, bob=0;
        for(int i=0 ; i<n ; i++){
            if(i%2==0){
                if(a[i]%2==0) alice += a[i];
            }
            else{
                if(a[i]%2==1) bob+=a[i];
            }
        }
        if(alice>bob) cout<<"Alice\n";
        else if(bob>alice) cout<<"Bob\n";
        else               cout<<"Tie\n";
 
    }
 
return 0;
}
