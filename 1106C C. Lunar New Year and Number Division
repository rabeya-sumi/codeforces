#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
 
int main()
{
    int n, a[300005];
    cin>>n;
    for(int i=0 ; i<n ; i++) cin>>a[i];
    sort(a, a+n);
    ll sum=0;
    for(int i=0 ; i<n/2 ; i++)
    {
        sum += (a[i]+a[n-1-i]) * (a[i]+a[n-1-i]);
    }
    cout<<sum<<"\n";
 
 
return 0;
}
