#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    int t, a[300005], q, n, x;
    cin>>n>>q;
    vector<int>p(55);
    for(int i=1 ; i<=n ; i++) cin>>a[i];
    for(int i=1 ; i<=n ; i++){
        if(p[a[i]]==0) p[a[i]]=i;
    }
    while(q--){
        cin>>x;
        cout<<p[x]<<' ';
        for(int i=1 ; i<=50 ; i++){
            if(p[i]!=0 && p[i]<p[x]) p[i]++;
        }
        p[x]=1;
    }
 
 
 
return 0;
}
